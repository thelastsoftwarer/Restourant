
public class Medium_Coke extends Coke {
	public String name() {  
        return "500 ml Coke";    
   }  
 
     
   public String size() {  
        
       return "Medium Size";  
   }  
 
    
   public int price() {  
     
       return  10;  
   }  
   public  int discount() {
   	return 10;
   } 
}
