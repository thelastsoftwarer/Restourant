
public class Small_Coke extends Coke{
	  
    public String name() {  
         return "300 ml Coke";    
    }  
  
      
    public String size() {  
         
        return "Small Size";  
    }  
  
     
    public int price() {  
      
        return  5;  
    }  
    public  int discount() {
    	return 10;
    } 
}
